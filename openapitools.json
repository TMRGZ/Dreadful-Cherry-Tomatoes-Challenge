{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "v2.0": {
        "generatorName": "typescript-angular",
        "output": "gen",
        "inputSpec": "api/movie-holder.spec.yaml",
        "additionalProperties": {
          "modelSuffix": "Dto",
          "modelFileSuffix": "Dto",
          "npmName": "restClient",
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
