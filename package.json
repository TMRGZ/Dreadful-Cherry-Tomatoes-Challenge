{
  "name": "skeleton-ts-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "yarn run generate:api",
    "start": "ng serve --host 0.0.0.0 --port 3000",
    "prebuild": "yarn run generate:api",
    "build": "ng build",
    "watch": "ng build --watch --browsers=ChromeHeadlessCI --configuration development",
    "test": "ng test --no-watch --browsers=ChromeHeadlessCI",
    "coverage": "ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI",
    "generate:api": "openapi-generator-cli generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.3",
    "@angular/common": "^16.0.3",
    "@angular/compiler": "^16.0.3",
    "@angular/core": "^16.0.3",
    "@angular/forms": "^16.0.3",
    "@angular/platform-browser": "^16.0.3",
    "@angular/platform-browser-dynamic": "^16.0.3",
    "@angular/router": "^16.0.3",
    "@ng-bootstrap/ng-bootstrap": "15.0.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "^16.0.3",
    "@angular/localize": "^16.0.3",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/jasmine": "~4.3.2",
    "jasmine-core": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.4"
  }
}
